<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="adsp.template"
  version="0.0.1"
  name="Audio DSP Template"
  provider-name="Team XBMC">
  <requires>
    <c-pluff version="0.1"/>
    <import addon="xbmc.adsp" version="0.0.1"/>
  </requires>
  <extension
    point="xbmc.adspaddon"
    library_linux="adsp.template.so"
    library_osx="adsp.template.dylib"
    library_wingl="adsp.template.dll"
    library_windx="adsp.template.dll"
    library_android="libadsp.template.so" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Template for a Audio DSP Addon</summary>
    <description lang="en">Loopback, do no Signal Processing</description>
    <disclaimer lang="en">Just contains stubs</disclaimer>
    <platform>all</platform>
  </extension>
</addon>
